# GitLab Configuration
GITLAB_ROOT_PASSWORD=secure-root-password-123
GITLAB_TOKEN=glpat-xxxxxxxxxxxxxxxxxxxx
GITLAB_API_URL=http://gitlab/api/v4
GITLAB_PROJECT_ID=

# GitLab MCP Server Configuration
MCP_SERVER_PORT=3002
GITLAB_READ_ONLY_MODE=false
USE_GITLAB_WIKI=true
USE_MILESTONE=true
USE_PIPELINE=true
NODE_ENV=production

# Optional: Advanced MCP Server Modes
# SSE=false
# STREAMABLE_HTTP=false

# Database Configuration
POSTGRES_DB=gitlabhq_production
POSTGRES_USER=gitlab
POSTGRES_PASSWORD=secure-database-password-123

# Cache Configuration
CACHE_TTL=3600

# Logging Configuration
LOG_LEVEL=info

# Optional: PDF Export (set to 'true' to enable)
ENABLE_PDF_EXPORT=false
